	<queryString>
		<![CDATA[select md.$P!{Id_doc} as id_codigos, per.nombre, mt.materia,
	case
	when md.documento isnull=true then 'Faltante'
	when md.documento isnull=false then 'Subido'
	end
	as estado
	from $P!{Tabla} md 
    join periodo_academico per on md.id_periodo=per.id_periodo
    join materia mt on md.id_materia=mt.codigo
    where  md.$P!{Id_doc} in($P!{Codigos});]]>
	</queryString>
