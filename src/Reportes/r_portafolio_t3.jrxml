	<queryString>
		<![CDATA[select n.$P!{Id_doc} as iddoc, n.descripcion as des, ps.nombres||' '||ps.apellidos as docente,
                    per.nombre as periodo, mt.materia as materia, ad.ciclo as ciclo, ad.jornada as jornada,
					ad.paralelo as paralelo, case
	when n.documento isnull=true then 'Faltante'
	when n.documento isnull=false then 'Subido'
	end
	as estado
                    from $P!{Tabla} n
                    join asignacion_docentes ad on n.id_asignacion=ad.id_asignacion
                    join persona ps on ad.identificacion=ps.cedula
                    join periodo_academico per on ad.periodo=cast(per.id_periodo as varchar)
                    join materia mt on ad.asignatura=mt.codigo
                    where n.$P!{Id_doc} in($P!{Codigos});]]>
	</queryString>
