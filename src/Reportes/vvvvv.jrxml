	<subDataset name="Table Dataset 1" uuid="f2b1b656-6ba7-416f-878e-65ff82e9381b"/>
	<parameter name="ciclo" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[select p.apellidos||' '||p.nombres as "DOCENTE", case 
 	when jornada='MATUTINA' then
		'M'||ad.ciclo||paralelo
	when jornada='VESPERTINA' then
		'V'||ad.ciclo||paralelo
	when jornada='NOCTURNA' then
		'N'||ad.ciclo||paralelo
end as "CICLO" , materia,p2.nombres||' '||p2.apellidos as "RESPONSABLE",fecha AS "PLAN",hora_guias AS "GUIA", c.nombre,d.numero,pa.nombre
from persona p join asignacion_docentes ad on p.cedula=ad.identificacion
join documentacion d on d.id_asignacion=ad.id_asignacion||''
join materia m on ad.asignatura=m.codigo
join area_carrera a on m.area=a.id_area||''
join persona p2 on a.responsable=p2.cedula
join carrera c on c.codigo=m.nombre
join periodo_academico pa on pa.id_periodo||''=ad.periodo 
where  case 
 	when jornada='MATUTINA' then
		'M'||ad.ciclo||paralelo
	when jornada='VESPERTINA' then
		'V'||ad.ciclo||paralelo
	when jornada='NOCTURNA' then
		'N'||ad.ciclo||paralelo
end=$P{ciclo}]]>
	</queryString>
