	<parameter name="index" class="java.lang.String"/>
	<queryString>
		<![CDATA[select ac.identificacion, p.nombres || ' ' ||  p.apellidos AS nombres, m.materia, m.ciclo, ac.paralelo
from carrera c
join materia m
on m.nombre = c.codigo
join asignacion_docentes ac 
on m.codigo = ac.asignatura
join persona p 
on p.cedula = ac.identificacion
where upper (c.nombre) like upper ('%%')]]>
	</queryString>
