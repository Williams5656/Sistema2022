	<queryString>
		<![CDATA[SELECT
     docente."cedula" AS docente_cedula,
     docente."titulo" AS docente_titulo,
     docente."abreviatura" AS docente_abreviatura,
     docente."tiempo" AS docente_tiempo,
     docente."ocupacion" AS docente_ocupacion,
     docente."estado" AS docente_estado,
     persona."cedula" AS persona_cedula,
